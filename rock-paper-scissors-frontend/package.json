{
  "name": "rock-paper-scissors-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --open --configuration development",
    "dev-build": "ng build --configuration development",
    "prod-build": "ng build --configuration production",
    "prod-gh-pages-build": "ng build --configuration production --base-href /RockPaperScissors/",
    "hooks-setup": "cd .. && husky install rock-paper-scissors-frontend/.husky && rm -R rock-paper-scissors-frontend/.husky/_",
    "prettier": "npx prettier --write --ignore-unknown src/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.7",
    "@angular/cli": "15.0.1",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jasmine": "~4.0.0",
    "husky": "^8.0.1",
    "prettier": "2.7.1",
    "typescript": "~4.7.2"
  }
}
